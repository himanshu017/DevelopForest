@model DeveloperForest.Model.CategoryModel

<div class="subnav__boxcon" data-parent="@Model.CategoryId" id="@Model.CategoryName" style="display: block;">
    <ul class="subnav__box">
        @for (int i = 0; i < Model.ModelList.Count; i++)
        {
            <li class="subnav__list hoverdropdownlik">
                <a class="subnav__link" onclick="GetTheme(this)" data-id="@Model.ModelList[i].SubCategoryId" href="#">@Model.ModelList[i].SubCategoryName</a>
            </li>
        }
    </ul>
</div>

